{
  "name": "@dreamer/webrtc",
  "version": "1.0.0-beta.2",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts",
    "./client": "./src/client/mod.ts",
    "./server": "./src/server/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "LICENSE.md",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**",
      "**/examples/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@1.0.0-beta.19",
    "@dreamer/socket-io": "jsr:@dreamer/socket-io@1.0.0-beta.2",
    "@dreamer/logger": "jsr:@dreamer/logger@1.0.0-beta.4",
    "@dreamer/test": "jsr:@dreamer/test@1.0.0-beta.37"
  },
  "nodeModulesDir": "auto"
}
